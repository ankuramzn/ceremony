<h1>Listing enrollments</h1>

<table>
  <tr>
    <th>Student</th>
    <th>Program</th>
    <th>Ceremony</th>
    <th>Guest count</th>
    <th>Requested prof</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @enrollments.each do |enrollment| %>
  <tr>
    <td><%= enrollment.student_id %></td>
    <td><%= enrollment.program_id %></td>
    <td><%= enrollment.ceremony_id %></td>
    <td><%= enrollment.guest_count %></td>
    <td><%= enrollment.requested_prof %></td>
    <td><%= link_to 'Show', enrollment %></td>
    <td><%= link_to 'Edit', edit_enrollment_path(enrollment) %></td>
    <td><%= link_to 'Destroy', enrollment, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Enrollment', new_enrollment_path %>
